{
    "name": "visuellverstehen/statamic-anchor-navigation",
    "description": "A Statamic tag for creating an anchor nav for all headings generated by the bard editor.",
    "keywords": [ "statamic", "addon", "bard", "navigation", "anchor", "v3", "v4" ],
    "license": "MIT",
    "type": "statamic-addon",
    "autoload": {
        "psr-4": {
            "VV\\AnchorNavigation\\": "src"
        }
    },
    "authors": [
        {
            "name": "visuellverstehen"
        }
    ],
    "support": {
        "email": "hello@visuellverstehen.de"
    },
    "extra": {
        "statamic": {
            "name": "Statamic Anchor Navigation",
            "description": "A Statamic tag for creating an anchor nav for all headings generated by the bard editor."
        },
        "laravel": {
            "providers": [
                "VV\\AnchorNavigation\\ServiceProvider"
            ]
        }
    },
    "require": {
        "statamic/cms": "^3.4 || ^4.0"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    }
}
